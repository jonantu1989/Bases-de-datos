<web-app xmlns="http://java.sun.com/xml/ns/javaee"
	      xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	      xsi:schemaLocation="http://java.sun.com/xml/ns/javaee
	      http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd"
	      version="3.0">
  <display-name>Archetype Created Web Application</display-name>
  <listener>
  	<listener-class>com.ipartek.formacion.listeners.Inicializacion</listener-class>
  </listener>
  <servlet>
  	<description>
  	</description>
  	<display-name>LoginServlet</display-name>
  	<servlet-name>LoginServlet</servlet-name>
  	<servlet-class>com.ipartek.formacion.carrito.servlets.LoginServlet</servlet-class>
  </servlet>
  <servlet>
  	<description>
  	</description>
  	<display-name>AltaServlet</display-name>
  	<servlet-name>AltaServlet</servlet-name>
  	<servlet-class>com.ipartek.formacion.carrito.servlets.AltaServlet</servlet-class>
  </servlet>
  <servlet>
  	<description>
  	</description>
  	<display-name>UsuarioCrudServlet</display-name>
  	<servlet-name>UsuarioCrudServlet</servlet-name>
  	<servlet-class>com.ipartek.formacion.carrito.servlets.UsuarioCrudServlet</servlet-class>
  </servlet>
  <servlet>
  	<description>
  	</description>
  	<display-name>ProductoCrudServlet</display-name>
  	<servlet-name>ProductoCrudServlet</servlet-name>
  	<servlet-class>com.ipartek.formacion.carrito.servlets.ProductoCrudServlet</servlet-class>
  </servlet>
